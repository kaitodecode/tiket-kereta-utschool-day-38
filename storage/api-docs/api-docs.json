{
    "openapi": "3.0.0",
    "info": {
        "title": "Train Ticket Booking API",
        "description": "API Documentation for Train Ticket Booking System",
        "contact": {
            "name": "API Support",
            "email": "admin@example.com"
        },
        "license": {
            "name": "Apache 2.0",
